source("EGO.R")
 f = DiceKriging::branin
 options = list(search_min='true',initBatchSize='4',batchSize='4',iterations='10',bounds='true',trend='y~1',covtype='matern3_2',liar='min')
 
 ego = EGO(options)
 
 X0 = getInitialDesign(ego,3)
 Y0 = f(X0)
 
 Xi = X0
 Yi = Y0
 
 finished = FALSE
 while (!finished) {
   Xj = getNextDesign(ego,Xi,Yi)
   if (is.null(Xj) | length(Xj) == 0) {
     finished = TRUE
   } else {
     Yj = f(Xj)
     Xi = rbind(Xi,Xj)
     Yi = rbind(Yi,Yj)
   }
 }
 
 print(displayResults(ego,Xi,Yi))
install.packages("DiveView")
q()
